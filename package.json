{
  "name": "bbqpi-server",
  "version": "1.0.0",
  "description": "A node and firebase based service to install on a Raspberry Pi for controlling a charcoal bbq fan blower and temperature sensors.",
  "main": "src/server.js",
  "scripts": {
    "test": "jest --coverage && codecov",
    "posttest": "eslint .",
    "stryker": "stryker run stryker.conf.js",
    "jest": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mshogren/bbqpi-server.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mshogren/bbqpi-server/issues"
  },
  "homepage": "https://github.com/mshogren/bbqpi-server#readme",
  "dependencies": {
    "firebase": "^3.6.9",
    "jfs": "^0.2.6",
    "mcp-spi-adc": "^0.4.0",
    "pi-gpioutil": "0.0.3",
    "request": "^2.79.0",
    "web-push": "^3.2.2"
  },
  "devDependencies": {
    "codecov": "^1.0.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^19.0.0",
    "jest-cli": "^19.0.2",
    "stryker": "^0.5.8",
    "stryker-api": "^0.4.2",
    "stryker-jest-runner": "0.0.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "/.stryker-tmp/"
    ]
  }
}
